/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include "rockchip/px30.dtsi"

/ {
	model = "Vimar P451";
	compatible = "vimar,p451", "rockchip,px30";

	chosen {
		stdout-path = "serial2:1500000n8";
	};

	aliases {
		rtc0 = &hym8563;
		rtc1 = &rk809;
	};

	emmc_pwrseq: emmc-pwrseq {
		compatible = "mmc-pwrseq-emmc";
		pinctrl-0 = <&emmc_reset>;
		pinctrl-names = "default";
		reset-gpios = <&gpio1 RK_PB3 GPIO_ACTIVE_HIGH>;
	};

	vcc5v0_sys: vccsys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	vcc13v0: vcc13v0 {
		compatible = "regulator-fixed";
		regulator-name = "vcc13v0";
		gpio = <&gpio3 RK_PD1 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		//regulator-always-on;
		//regulator-boot-on;
		startup-delay-us = <1000>;
		regulator-min-microvolt = <13000000>;
		regulator-max-microvolt = <13000000>;
	};

        lcd_supply: lcd-fixedregulator {
                compatible = "regulator-fixed";
                regulator-name = "lcd-fixed-supply";
                regulator-min-microvolt = <3300000>;
                regulator-max-microvolt = <3300000>;
                gpio = <&gpio3 RK_PA7 GPIO_ACTIVE_HIGH>;
                startup-delay-us = <1000>;
                enable-active-high;
                //regulator-boot-on;
        };

	panel_backlight: panel-backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm7 0 25000 0>;
		brightness-levels = <0x00 0x06 0x0D 0x10 0x15 0x20 0x30 0x40
				     0x50 0x60 0x70 0x80 0x90 0xB0 0xD0 0xFF>;
		default-brightness-level = <14>;
		power-supply = <&vcc13v0>;
	};

	panel: panel {
		compatible = "satoz,sat050at40h12r2", "simple-panel";
		power-supply = <&lcd_supply>;
		enable-gpios = <&gpio3 RK_PA4 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio3 RK_PA3 GPIO_ACTIVE_LOW>;
		backlight = <&panel_backlight>;
		data-mapping = "vesa-24";
		width-mm = <108>;
		height-mm = <65>;
		label = "Vimar LCD";
		status = "okay";

		port@0 {
			panel_in: endpoint@0 {
				remote-endpoint = <&lvds_out>;
			};
		};
	};

	tw9900_supply: tw9900-fixedregulator {
		compatible = "regulator-fixed";
		regulator-name = "tw9900-fixed-supply";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio3 RK_PD0 GPIO_ACTIVE_HIGH>;
		startup-delay-us = <1000>;
		enable-active-high;
	};

	wifi_pwrseq: wifi_pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
		post-power-on-delay-ms = <200>;
	};
};

&cpu0 {
	cpu-supply = <&vdd_arm>;
};

&cpu1 {
	cpu-supply = <&vdd_arm>;
};

&cpu2 {
	cpu-supply = <&vdd_arm>;
};

&cpu3 {
	cpu-supply = <&vdd_arm>;
};

&sdio {
	status = "okay";
	bus-width = <4>;
	#address-cells = <1>;
	#size-cells = <0>;
	mmc-pwrseq = <&wifi_pwrseq>;

	brcmf: wifi@1 {
		reg = <0>;
		compatible = "brcm,bcm4329-fmac";
	};
};

&emmc {
	bus-width = <8>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	non-removable;
	mmc-pwrseq = <&emmc_pwrseq>;
	vmmc-supply = <&vcc_3v0>;
	vqmmc-supply = <&vccio_flash>;
	status = "okay";
};

&gpu {
	status = "okay";
	mali-supply = <&vdd_log>;
};

&i2c0 {
	status = "okay";

	rk809: pmic@20 {
		compatible = "rockchip,rk809";
		reg = <0x20>;
		interrupt-parent = <&gpio0>;
		interrupts = <7 IRQ_TYPE_LEVEL_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&pmic_int>;
		rockchip,system-power-controller;
		wakeup-source;
		#clock-cells = <0>;
		clock-output-names = "pmic-xout32k";

		vcc1-supply = <&vcc5v0_sys>;
		vcc2-supply = <&vcc5v0_sys>;
		vcc3-supply = <&vcc5v0_sys>;
		vcc4-supply = <&vcc5v0_sys>;
		vcc5-supply = <&vcc3v3_sys>;
		vcc6-supply = <&vcc3v3_sys>;
		vcc7-supply = <&vcc3v3_sys>;
		vcc8-supply = <&vcc3v3_sys>;
		vcc9-supply = <&vcc5v0_sys>;

		regulators {
			vdd_log: DCDC_REG1 {
				regulator-name = "vdd_log";
				regulator-min-microvolt = <950000>;
				regulator-max-microvolt = <1350000>;
				regulator-ramp-delay = <6001>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <950000>;
				};
			};

			vdd_arm: DCDC_REG2 {
				regulator-name = "vdd_arm";
				regulator-min-microvolt = <950000>;
				regulator-max-microvolt = <1350000>;
				regulator-ramp-delay = <6001>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-off-in-suspend;
					regulator-suspend-microvolt = <950000>;
				};
			};

			vcc_ddr: DCDC_REG3 {
				regulator-name = "vcc_ddr";
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
				};
			};

			vcc_3v0: vcc_rmii: DCDC_REG4 {
				regulator-name = "vcc_3v0";
				regulator-min-microvolt = <3000000>;
				regulator-max-microvolt = <3000000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3000000>;
				};
			};

			vcc3v3_sys: DCDC_REG5 {
				regulator-name = "vcc3v3_sys";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3300000>;
				};
			};

			vcc_1v0: LDO_REG1 {
				regulator-name = "vcc_1v0";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1000000>;
				};
			};

			vcc_1v8: vccio_flash: vccio_sdio: LDO_REG2 {
				regulator-name = "vcc_1v8";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1800000>;
				};
			};

			vdd_1v0: LDO_REG3 {
				regulator-name = "vdd_1v0";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1000000>;
				};
			};

			vcc3v0_pmu: LDO_REG4 {
				regulator-name = "vcc3v0_pmu";
				regulator-min-microvolt = <3000000>;
				regulator-max-microvolt = <3000000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3000000>;
				};
			};

			vccio_sd: LDO_REG5 {
				regulator-name = "vccio_sd";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3300000>;
				};
			};

			vcc_sd: LDO_REG6 {
				regulator-name = "vcc_sd";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3300000>;
				};
			};

			vcc2v8_dvp: LDO_REG7 {
				regulator-name = "vcc2v8_dvp";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2800000>;
				regulator-boot-on;

				regulator-state-mem {
					regulator-off-in-suspend;
					regulator-suspend-microvolt = <2800000>;
				};
			};

			vcc1v8_dvp: LDO_REG8 {
				regulator-name = "vcc1v8_dvp";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1800000>;
				};
			};

			vcc1v5_dvp: LDO_REG9 {
				regulator-name = "vcc1v5_dvp";
				regulator-min-microvolt = <1500000>;
				regulator-max-microvolt = <1500000>;
				regulator-boot-on;

				regulator-state-mem {
					regulator-off-in-suspend;
					regulator-suspend-microvolt = <1500000>;
				};
			};

			vcc5v0_host: SWITCH_REG1 {
				regulator-name = "vcc5v0_host";
				regulator-always-on;
				regulator-boot-on;
			};

			vcc3v3_lcd: SWITCH_REG2 {
				regulator-name = "vcc3v3_lcd";
				regulator-boot-on;
			};
		};
	};

	hym8563: rtc@51 {
		compatible = "haoyu,hym8563";
		pinctrl-names = "default";
		pinctrl-0 = <&hym8563_int>;
		reg = <0x51>;

		interrupt-parent = <&gpio0>;
		interrupts = <RK_PB5 IRQ_TYPE_EDGE_FALLING>;
		#clock-cells = <0>;
	};
};

&i2c1 {
	status = "okay";

	touchscreen: touchscreen@14 {
		compatible = "goodix,gt911";
		reg = <0x14>;
		interrupt-parent = <&gpio0>;
		/* Sensitivity/polarity matches touchscreen config registers. */
		interrupts = <RK_PA5 IRQ_TYPE_EDGE_FALLING>;
		irq-gpios = <&gpio0 RK_PA5 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;
		VDDIO-supply = <&vcc_3v0>;
	};
};

&i2c2 {
	status = "okay";

	clock-frequency = <100000>;

	i2c-scl-falling-time-ns = <50>;
	i2c-scl-rising-time-ns = <300>;
	
	tw9900: tw9900@44 {
		compatible = "techwell,tw9900";
		reg = <0x44>;
		dvdd-supply = <&tw9900_supply>;
		reset-gpio = <&gpio2 RK_PB5 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&cif_clkout_m0 &dvp_d2d9_m0
			     &dvp_d0d1_m0>;
		status = "okay";
		port {
			tw9900_out: endpoint {
				remote-endpoint = <&cif_in>;
			};
		};
	};
};

&i2s1_2ch {
	status = "okay";
};

&io_domains {
	status = "okay";

	vccio1-supply = <&vccio_sdio>;
	vccio2-supply = <&vccio_sd>;
	vccio3-supply = <&vcc_3v0>;
	vccio4-supply = <&vcc3v0_pmu>;
	vccio5-supply = <&vcc_3v0>;
	vccio6-supply = <&vccio_flash>;
};

&pinctrl {
	emmc {
		emmc_reset: emmc-reset {
			rockchip,pins = <1 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PA7 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		soc_slppin_gpio: soc_slppin_gpio {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_GPIO &pcfg_output_low>;
		};

		soc_slppin_slp: soc_slppin_slp {
			rockchip,pins =
				<0 RK_PA4 1 &pcfg_pull_none>;
		};

		soc_slppin_rst: soc_slppin_rst {
			rockchip,pins =
				<0 RK_PA4 2 &pcfg_pull_none>;
		};
	};

	rtc {
		hym8563_int: hym8563_int {
			rockchip,pins = <1 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

&pmu_io_domains {
	status = "okay";

	pmuio1-supply = <&vcc3v0_pmu>;
	pmuio2-supply = <&vcc3v0_pmu>;
};

&pwm7 {
	status = "okay";
};

&saradc {
	vref-supply = <&vcc_1v8>;
	status = "okay";
};

&sdmmc {
	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	card-detect-delay = <800>;
	sd-uhs-sdr12;
	sd-uhs-sdr25;
	sd-uhs-sdr50;
	sd-uhs-sdr104;
	vmmc-supply = <&vcc_sd>;
	vqmmc-supply = <&vccio_sd>;
	status = "disabled";
};

&u2phy {
	status = "okay";

	u2phy_otg: otg-port {
		status = "okay";
	};
};

/* AP6212A WiFi/Bluetooth module */
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_xfer &uart1_cts>;
	status = "okay";
};

/* CN12 connector (conflicts with SD card) */
&uart2 {
	status = "ok";
};

/* Console (available on the SoM) */
&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart3m0_xfer>;
	status = "okay";
};

/* MCU */
&uart5 {
	status = "okay";
};

&usb20_otg {
	status = "okay";
};

&display_subsystem {
	status = "okay";
};

&dsi_dphy {
	status = "okay";
};

&lvds {
	status = "okay";

	port@1 {
		reg = <1>;

		lvds_out: endpoint {
			remote-endpoint = <&panel_in>;
		};
	};
};

&cif {
	status = "okay";

	port {
		cif_in: endpoint {
			remote-endpoint = <&tw9900_out>;
		};
	};
};

&vopb {
	status = "okay";
};

&vopb_mmu {
	status = "okay";
};

&vopl {
	status = "okay";
};

/*
&vip_mmu {
	status = "okay";
};
*/

&vopl_mmu {
	status = "okay";
};
