				"alt_pedale_2",
				"dec_ucomb",
				"tor_in_9",
				"tor_in_10",
				"alt_aith",
				"tor_out_audio_1",
				"tor_out_audio_2",
				"tor_out_audio_3",
				"tor_out_audio_4",
				"tor_out_5",
				"tor_out_6",
				"none",
				"none",
				"alt_bp",
				"tor_in_13",		// Alternat 1
				"tor_in_14",		// Alternat 2
				"alt_ucomb",
				"pres_ucomb";
			codec = "Audio In 1",
				"Audio In 2",
				"Audio In 3",
