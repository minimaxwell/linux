				gpiofav: csfavgw@7 {
					compatible = "gpio,max7301", "cs,mia-fav";
					spi-max-frequency = <2000000>;
					reg = <7>;
					spi-cs-high;
					#gpio-cells = <2>;
					gpio-controller;
					fav-id = <4>;
