		GPIO_FAV_NVCS {
			compatible = "s3k,gpios-def-fav", "cs,mia-fav";
			fav-id = <3 5 6>;
			names = "alt_umain_a",
				"alt_umain_b",
				"alt_ucasque_a",
				"alt_ucasque_b";
			pres  = "pres_ucasque_a",
				"pres_umain_a",
				"pres_ucasque_b",
				"pres_umain_b";
			codec = "Micro Casque_a",
				"Micro Main_a",
				"Micro Casque_b",
				"Micro Main_b",
				"Ec. droite Casque_a",
				"Ec. gauche Casque_a",
