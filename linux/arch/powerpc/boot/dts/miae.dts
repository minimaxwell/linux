		IDENT_EQUIPT_NVCS {
			compatible = "ident-equipt-fav", "cs,mia-fav";
			fav-id = <3 5 6>;
			infos = "22",	/* nombre d'équipements possibles */
				/* bornes min et max, niveaux in et out en dB, croissement TS, type casque */
