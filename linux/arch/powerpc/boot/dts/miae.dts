		GPIO_FAV_CLA {
			compatible = "s3k,gpios-def-fav", "cs,mia-fav";
			fav-id = <7>;
			names = "alt_umain_a",
				"alt_umain_b",
				"alt_ucasque_a",
				"alt_ucasque_b";
			pres  = "pres_ucasque_a",
				"pres_umain_a",
				"pres_ucasque_b",
				"pres_umain_b";
			codec = "Micro Casque_a",
				"Micro Main_a",
				"Micro Casque_b",
				"Micro Main_b",
				"Ec. droite Casque_a",
				"Ec. gauche Casque_a",
				"Ec. droite Casque_b",
				"Ec. gauche Casque_b";
		};

		GPIO_CA_NVCS {
			compatible = "s3k,gpios-def-ca", "cs,mia-far";
			far-id = <1>;
			names = "tor_in_audio_1",
				"tor_in_audio_2",
