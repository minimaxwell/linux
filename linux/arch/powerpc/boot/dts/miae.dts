				"Ec. gauche Casque_b";
		};

		GPIO_CA_GW {
			compatible = "s3k,gpios-def-ca", "cs,mia-far";
			far-id = <4>;
			names = "tor_in_1",
				"tor_in_2",
				"tor_in_3",
				"tor_in_4",
				"tor_in_5",
				"tor_in_6",
				"tor_in_7",
